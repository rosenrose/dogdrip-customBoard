{
    "author": "rosenrose",
    "manifest_version": 2,
    "name": "Dogdrip Custom Board",
    "description": "Dogdrip Custom Board",
    "version": "1.1.2",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "options.html"
    },
    "permissions": [
        "storage"
    ],
    "options_page": "options.html",
    "content_scripts": [{
        "matches": [
            "*://www.dogdrip.net/"
        ],
        "js": [
            "script.js"
        ]
    }],
    "icons": {
        "128": "icon.png"
    }
}